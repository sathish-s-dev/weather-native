{
	"expo": {
		"scheme": "acme",
		"userInterfaceStyle": "automatic",
		"orientation": "default",
		"web": {
			"output": "static",
			"bundler": "metro"
		},
		"plugins": [
			[
				"expo-router",
				{
					"origin": "https://n"
				}
			],
			[
				"expo-location",
				{
					"locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
				}
			]
		],
		"name": "weather-app",
		"slug": "weather-app"
	}
}
